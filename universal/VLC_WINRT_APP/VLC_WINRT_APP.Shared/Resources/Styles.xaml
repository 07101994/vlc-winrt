                            </Border>
                            <Rectangle Grid.Column="3"
                                       Width="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=TemplateSettings.EllipseOffset}" />
                            <Border x:Name="B4"
                                    Grid.Column="2"
