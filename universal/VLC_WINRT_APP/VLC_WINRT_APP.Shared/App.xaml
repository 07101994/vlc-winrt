<Application
    x:Class="VLC_WINRT_APP.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:VLC_WINRT_APP"
    xmlns:viewModels="using:VLC_WINRT.ViewModels"
    xmlns:dataTemplateSelectors="using:VLC_WINRT.Utility.DataTemplateSelectors"
    xmlns:converters="using:VLC_WINRT.Utility.Converters"
    xmlns:converters1="using:VLC_WINRT_APP.Utility.Converters">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Colors.xaml" />
                <ResourceDictionary Source="Resources/Converters.xaml" />
                <ResourceDictionary Source="Resources/Styles.xaml" />
                <ResourceDictionary Source="Resources/Templates.xaml" />
                <ResourceDictionary Source="Resources/DataTemplateSelectors.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <viewModels:Locator x:Key="Locator" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
